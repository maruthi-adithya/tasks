
import java.util.*;
import java.lang.*;
import java.io.*;

/* Name of the class has to be "Main" only if the class is public. */
class Ideone
{
	int a;  
   static String b="cool";  
   static String c ="ITS";  
     
   Ideone(int r){  
   a = r;  
  
   }  
 void display (){System.out.println(a+" "+b+" "+c);}  

	public static void main (String[] args) 
	{
		// your code goes here
		 Ideone s1 = new Ideone(111);
		 s1.display();
	}
}
